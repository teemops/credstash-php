    "keywords": ["aws", "dynamodb", "kms"],
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        },
        {
            "name": "Ben Fellows",
            "email": "kiwifellows@gmail.com"
        }
    ],
    "require": {
        "aws/aws-sdk-php": "^3.9",
        "ext-openssl": "*",
        "php": "^5.5.9 || ^7.0",
        "symfony/polyfill-php56": "^1.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "symfony/console": "^2.7 || ^3.0",
        "symfony/yaml": "^2.7 || ^3.0"
    },
    "autoload": {
        "psr-4": {
